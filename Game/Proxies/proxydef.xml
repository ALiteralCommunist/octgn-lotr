<?xml version="1.0" encoding="utf-8" ?>
<templates>
  <blocks>
    <!-- Free People card -->
    <block type="text" id="name">
      <location x="70" y="23"/>
      <text color="#000000" size="18"/>
      <wordwrap height="20" width="180" shrinktofit="True"/>
    </block>
    <block type="text" id="subname">
      <location x="70" y="46"/>
      <text color="#000000" size="13"/>
    </block>
    <block type="text" id="types">
      <location x="60" y="290"/>
      <text color="#000000" size="16"/>
      <wordwrap height="20" width="235" align="center" valign="center" shrinktofit="True"/>
    </block>
    <block type="text" id="twilight">
      <location x="18" y="18"/>
      <text color="#000000" size="25"/>
      <wordwrap height="45" width="45" align="center" valign="center"/>
    </block>
    <block type="text" id="strength">
      <location x="22" y="314"/>
      <text color="#FFFFFF" size="25"/>
      <wordwrap height="45" width="45" align="center" valign="center"/>
    </block>
    <block type="text" id="vitality">
      <location x="22" y="372"/>
      <text color="#FFFFFF" size="25"/>
      <wordwrap height="45" width="45" align="center" valign="center"/>
    </block>
    <block type="text" id="resistance">
      <location x="22" y="422"/>
      <text color="#FFFFFF" size="25"/>
      <wordwrap height="45" width="45" align="center" valign="center"/>
    </block>
    <block type="text" id="site">
      <location x="22" y="422"/>
      <text color="#FFFFFF" size="25"/>
      <wordwrap height="45" width="45" align="center" valign="center"/>
    </block>
    <block type="text" id="gametext">
      <location x="80" y="330"/>
      <text color="#000000" size="16"/>
      <wordwrap height="126" width="250" />
    </block>
    <!-- Shadow -->
    <block type="text" id="twilight_shadow">
      <location x="18" y="18"/>
      <text color="#FFFFFF" size="25"/>
      <wordwrap height="45" width="45" align="center" valign="center"/>
    </block>
    <!-- Site -->
    <block type="text" id="name_site">
      <location x="180" y="26"/>
      <text color="#000000" size="18"/>
      <wordwrap height="20" width="180" shrinktofit="True"/>
    </block>
    <block type="text" id="twilight_site">
      <location x="430" y="25"/>
      <text color="#FFFFFF" size="25"/>
      <wordwrap height="45" width="45" align="center" valign="center"/>
    </block>
    <block type="text" id="site_site">
      <location x="20" y="25"/>
      <text color="#FFFFFF" size="25"/>
      <wordwrap height="45" width="45" align="center" valign="center"/>
    </block>
    <block type="text" id="gametext_site">
      <location x="60" y="275"/>
      <text color="#000000" size="12"/>
      <wordwrap height="80" width="385" />
    </block>
    <!-- The One Ring -->
    <block type="text" id="subname_ring">
      <location x="95" y="280"/>
      <text color="#000000" size="18"/>
    </block>
    <block type="text" id="gametext_ring">
      <location x="95" y="330"/>
      <text color="#000000" size="16"/>
      <wordwrap height="126" width="250" />
    </block>
  </blocks>
  <!-- Free People -->
  <template src="Proxies/dwarven.jpg">
    <matches>
      <match name="Culture" value="Dwarven" />
    </matches>
    <textblocks>
      <link block="name"><property name="Name" /></link>
      <link block="subname"><property name="Subname" /></link>
      <link block="types"><property name="Type" /></link>
      <link block="twilight"><property name="Twilight" /></link>
      <link block="strength"><property name="Strength" /></link>
      <link block="vitality"><property name="Vitality" /></link>
      <link block="resistance"><property name="Resistance" /></link>
      <link block="gametext"><property name="Text" /></link>
